<map proto="1.4.2">
<name>Summit</name>
<version>1.1.2</version>
<objective>Capture the enemy wool!</objective>
<time result="objectives" max-overtime="10m" overtime="10m">25m</time>
<authors>
    <author uuid="b8add1ba-8e13-4673-bc73-4e3ed524d40e"/>
</authors>
<teams>
    <team id="blue-team" color="blue" max="5" max-overfill="5">Blue</team>
    <team id="red-team" color="red" max="5" max-overfill="5">Red</team>
</teams>
<kits>
    <kit id="default-kit">
        <effect>night vision</effect>
    </kit>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" show-unbreakable="false" material="iron sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="28" amount="40" material="arrow"/>
        <item slot="2" unbreakable="true" show-unbreakable="false" material="iron pickaxe">
            <enchantment>efficiency</enchantment>
        </item>
        <item slot="3" unbreakable="true" show-unbreakable="false" material="iron axe"/>
        <item slot="4" amount="64" material="glass"/>
        <item slot="5" amount="64" damage="1" material="wood"/>
        <item slot="6" amount="64" damage="1" material="wood"/>
        <item slot="7" material="golden apple"/>
        <item slot="8" material="water bucket"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <effect duration="5" amplifier="3">heal</effect>
        <effect duration="5" amplifier="3">damage resistance</effect>
    </kit>
</kits>
<spawns>
    <default kit="default-kit">
        <region yaw="180">
            <point>-88.5,33,95.5</point>
        </region>
    </default>
    <spawn team="blue-team" kit="spawn-kit">
        <region yaw="270">
            <point>-164.5,11,33.5</point>
        </region>
    </spawn>
    <spawn team="red-team" kit="spawn-kit">
        <region yaw="90">
            <point>-12.5,11,33.5</point>
        </region>
    </spawn>
</spawns>
<filters>
    <not id="blue-in-wr">
        <any>
            <team id="only-red">red-team</team>
            <material>chest</material>
        </any>
    </not>
    <not id="red-in-wr">
        <any>
            <team id="only-blue">blue-team</team>
            <material>chest</material>
        </any>
    </not>
    <material id="only-iron">iron block</material>
    <not id="deny-void">
        <void/>
    </not>
</filters>
<regions>
    <union id="spawns">
        <rectangle id="blue-spawn" min="-168,28" max="-148,39"/>
        <rectangle id="red-spawn" min="-29,28" max="-9,39"/>
    </union>
    <union id="wool-rooms">
        <rectangle id="blue-for-red" min="-165,81" max="-176,70"/> <!-- Blue must get -->
        <rectangle id="red-for-blue" min="-12,70" max="-1,81"/> <!-- Red must get -->
    </union>
    <union id="spawner-regions">
            <point id="spawner-spawn">-88.5,12,34.5</point>
            <cylinder id="spawner-player" base="-88.5,11,34.5" radius="2.5" height="5"/>
            <cuboid id="spawner-region" min="-89,11,34" max="-88,13,35"/>
    </union>
    <complement id="void">
        <everywhere/>
        <rectangle min="-148,28" max="-29,81"/>
    </complement>
    <apply enter="only-blue" region="blue-spawn" message="¡No puedes entrar en el spawn del enemigo!"/>
    <apply enter="only-red" region="red-spawn" message="¡No puedes entrar en el spawn del enemigo!"/>
    <apply enter="only-blue" region="red-for-blue" message="¡No puedes entrar en la sala de lana de tu propio equipo!"/>
    <apply enter="only-red" region="blue-for-red" message="¡No puedes entrar en la sala de lana de tu propio equipo!"/>
    <apply block="only-iron" region="spawns" message="¡Solo puedes romper bloques de hierro en el spawn!"/>
    <apply block="never" region="spawner-region" message="You may not destroy the spawners!"/>
    <apply block="blue-in-wr" use="only-blue" region="red-for-blue"/>
    <apply block="red-in-wr" use="only-red" region="blue-for-red"/>
    <apply block="deny-void" region="void" message="You may not build in the void!"/>
</regions>
<spawners>
    <spawner spawn-region="spawner-spawn" player-region="spawner-player" max-entities="1" delay="8s">
        <item material="golden apple"/>
    </spawner>
</spawners>
<renewables>
    <renewable rate="1" grow="false" particles="true" sound="true" avoid-entities="true" region="spawns" renew-filter="only-iron"/>
</renewables>
<wools wool-proximity-metric="none" monument-proximity-metric="closest block">
    <wool team="blue-team" color="orange" location="-5.5,10.5,75.5">
        <monument>
            <block>-166.5,12,33</block>
        </monument>
    </wool>
    <wool team="red-team" color="purple" location="-171.5,10.5,75.5">
        <monument>
            <block>-10.5,12,33.5</block>
        </monument>
    </wool>
</wools>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<itemremove>
    <item>sandstone</item>
    <item>wood</item>
    <item>golden carrot</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>IRON_PLATE</item>
</itemremove>
<itemkeep>
    <item>arrow</item>
</itemkeep>
<kill-reward>
    <item material="golden apple"/>
    <item amount="10" material="arrow"/>
    <item amount="32" damage="1" material="wood"/>
    <item amount="12" damage="2" material="glass"/>s
</kill-reward>
<maxbuildheight>21</maxbuildheight>
<respawn delay="4s" auto="true" blackout="true"/>
<hunger>
        <depletion>off</depletion>
</hunger>
<crafting>
    <disable>iron trapdoor</disable>
    <disable>minecart</disable>
    <disable>boat</disable>
</crafting>
</map>