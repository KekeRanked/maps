<map proto="1.4.2">
<name>Villa</name>
<version>1.1.2</version>
<objective>Capture the enemy wool!</objective>
<time result="objectives" max-overtime="10m" overtime="10m">20m</time>
<authors>
    <author uuid="6bec5c3a-4934-4b19-9aa8-c7c569820f7d"/>
</authors>
<teams>
    <team id="blue" color="blue" max="5" max-overfill="5">Blue</team>
    <team id="red" color="red" max="5" max-overfill="5">Red</team>
</teams>
<kits>
    <kit id="default-kit">
        <effect>night vision</effect>
    </kit>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" show-unbreakable="false" material="iron sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="28" amount="40" material="arrow"/>
        <item slot="2" unbreakable="true" show-unbreakable="false" material="iron pickaxe">
            <enchantment>efficiency</enchantment>
        </item>
        <item slot="3" unbreakable="true" show-unbreakable="false" material="iron axe">
            <enchantment>efficiency</enchantment>
        </item>
        <item slot="4" unbreakable="true" material="iron spade"/>
        <item slot="5" amount="64" damage="5" material="wood"/>
        <item slot="6" amount="64" material="glass"/>
        <item slot="7" material="golden apple"/>
        <item slot="32" amount="64" damage="5" material="wood"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate">
            <enchantment level="1">projectile_protection</enchantment>
        </chestplate>
        <effect duration="5" amplifier="3">heal</effect>
        <effect duration="5" amplifier="3">damage resistance</effect>
    </kit>
</kits>
<spawns>
    <default kit="default-kit">
        <region yaw="180">
            <point>-100,13,-57</point>
        </region>
    </default>
    <spawn team="blue" kit="spawn-kit">
        <region yaw="90">
            <point>-13,8,-85</point>
        </region>
    </spawn>
    <spawn team="red" kit="spawn-kit">
        <region yaw="-90">
            <point>-188,8,-85</point>
        </region>
    </spawn>
</spawns>
<filters>
    <not id="blue-in-wr">
        <any>
            <team id="only-red">red</team>
            <material>chest</material>
        </any>
    </not>
    <not id="red-in-wr">
        <any>
            <team id="only-blue">blue</team>
            <material>chest</material>
        </any>
    </not>
    <material id="only-iron">iron block</material>
    <material id="only-sand">sand</material>
    <not id="deny-void">
        <void/>
    </not>
</filters>
<regions>
    <union id="bridges">
            <rectangle min="-43,-88" max="-34,-81"/><!-- Blue bridge -->
            <rectangle min="-166,-88" max="-157,-81"/><!-- Red bridge -->
    </union>
    <rectangle id="buildable" min="-146,-117" max="-54,-52"/>
    <union id="not-buildable">
        <rectangle min="-122,-88" max="-78,-78"/><!-- Mid -->
        <rectangle min="-62,-88" max="-43,-81"/><!-- Blue -->
        <rectangle min="-157,-88" max="-138,-81"/><!-- Red -->
    </union>
    <union id="sand">
        <rectangle min="-104,-117" max="-96,-102"/>
    </union>
     <union id="spawns">
        <union id="blue-spawn">
            <rectangle min="-34,-95" max="-8,-74"/>
        </union>
        <union id="red-spawn">
            <rectangle min="-191,-95" max="-166,-74"/>
        </union>
    </union>
    <union id="gaps">
        <rectangle id="gaps-for-blue" min="-176,-64" max="-167,-52"/>
        <rectangle id="gaps-for-red" min="-33,-64" max="-24,-52"/>
    </union>
    <union id="wool-rooms">
        <rectangle id="purple-for-red" min="-194,-117" max="-182,-105"/> <!-- Blue must get -->
        <rectangle id="orange-for-blue" min="-18,-117" max="-6,-105"/> <!-- Red must get -->
    </union>
    <apply enter="only-blue" region="blue-spawn" message="You may not enter the enemy spawn!"/>
    <apply enter="only-red" region="red-spawn" message="You may not enter the enemy spawn!"/>
    <apply enter="only-blue" region="gaps-for-blue" message="¡No puedes entrar aquí!"/>
    <apply enter="only-red" region="gaps-for-red" message="¡No puedes entrar entrar aquí!"/>
    <apply enter="only-blue" region="purple-for-red" message="You may not enter your team's own wool room!"/>
    <apply enter="only-red" region="orange-for-blue" message="You may not enter your team's own wool room!"/>
    <apply block="only-iron" region="spawns" message="You may not edit the spawn!"/>
    <apply block="blue-in-wr" use="only-blue" region="purple-for-red"/>
    <apply block="red-in-wr" use="only-red" region="orange-for-blue"/>
    <apply block="always" region="bridges"/>
    <apply block-place="never" region="not-buildable" message="You may not edit the void!"/>
    <apply block-place="only-sand" region="sand" message="You may not edit the void!"/>
    <apply block="always" region="buildable"/>
    <apply block-place="deny-void" message="¡No puedes construir en el vacío!"/>
</regions>
<renewables>
    <renewable rate="1" grow="false" particles="true" sound="true" avoid-entities="true" region="spawns" renew-filter="only-iron"/>
</renewables>
<wools wool-proximity-metric="none" monument-proximity-metric="closest block">
    <wool team="blue" color="purple" location="-191,8,112">
        <monument>
            <block>-20,9,-85</block>
        </monument>
    </wool>
    <wool team="red" color="orange" location="-10,8,-112">
        <monument>
            <block>-181,9,-85</block>
        </monument>
    </wool>
</wools>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<itemremove>
    <item>wood</item>
    <item>seeds</item>
    <item>RED_ROSE</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>STONE_PLATE</item>
    <item>glass</item>
</itemremove>
<itemkeep>
    <item>arrow</item>
</itemkeep>
<kill-reward>
    <item material="golden apple"/>
    <item amount="32" damage="5" material="wood"/>
    <item amount="16" damage="2" material="glass"/>
    <item amount="10" material="arrow"/>
</kill-reward>
<maxbuildheight>21</maxbuildheight>
<respawn delay="4s" auto="true" blackout="true"/>
<hunger>
        <depletion>off</depletion>
</hunger>
<crafting>
    <disable>iron trapdoor</disable>
    <disable>minecart</disable>
    <disable>boat</disable>
</crafting>
</map>